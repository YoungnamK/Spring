<?xml version="1.0" encoding="UTF-8"?>
<!-- 스프링 설절파일  -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=
		"http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="rectangleBean" class="rectangle.exam.RectangleImpl">
		<constructor-arg>
			<value>10.0</value>
		</constructor-arg>
		<constructor-arg>
			<value>4.0</value>
		</constructor-arg>
	</bean>	
	
	<bean id="ladderBean" class="rectangle.exam.Ladder">
		<constructor-arg>
			<value>10</value>
		</constructor-arg>
		<constructor-arg>
			<value>20</value>
		</constructor-arg>
		<constructor-arg>
			<value>15</value>
		</constructor-arg>
	</bean>
</beans>